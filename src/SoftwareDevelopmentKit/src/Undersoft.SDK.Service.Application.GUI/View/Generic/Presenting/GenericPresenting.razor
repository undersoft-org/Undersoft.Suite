@using Undersoft.SDK.Service.Access;

@inherits LayoutComponentBase
@layout GenericLayout

@typeparam TLogo where TLogo : Icon, new()
@typeparam TMenu where TMenu : class, IOrigin, IInnerProxy
@typeparam TAccount where TAccount : class, IOrigin, IInnerProxy
@typeparam TModel where TModel : class, IOrigin, IInnerProxy, IAuthorization
@typeparam TValidator where TValidator : class, IValidator<IViewData<TModel>>

 
<GenericPresentingHeader Logo="@Logo" TMenu="TMenu" TModel="TModel" TAccount="TAccount" TValidator="TValidator" />

<CascadingValue Value="@Body">

    <GenericPresentingBody TMenu="TMenu" />

</CascadingValue>

<GenericPresentingFooter />

@code
{   
    public TLogo? Logo { get; set; } = new();
}