{
  "General": {
    "Name": "undersoft-scc-service-application-client",
    "Version": "v1",
    "BaseUrl": "http://undersoft-scc-service-application:26026",
    "TypeName": "Undersoft.SDK.Service.Application.Hosting.ApplicationHost"
  },
  "Repository": {
    "Clients": {
      "Undersoft.SCC.Service.Clients.ApplicationClient": {
        "ConnectionString": "http://127.0.0.1:16016/",
        "ClientProvider": "Open",
        "PoolSize": 6
      },
      "Undersoft.SCC.Service.Clients.AccessClient": {
        "ConnectionString": "http://127.0.0.1:16016/",
        "ClientProvider": "Open",
        "PoolSize": 6
      },
      "Undersoft.SCC.Service.Clients.EventClient": {
        "ConnectionString": "http://127.0.0.1:16016/",
        "ClientProvider": "Open",
        "PoolSize": 6
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.BrowserConsole" ],
    "WriteTo": [
      {
        "Name": "BrowserConsole"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "DataCache": {
    "Hours": "0",
    "Minutes": "45"
  }
}
